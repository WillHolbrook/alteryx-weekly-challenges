    $searchIcon.on('click', function(){
        $mobileSearch.addClass('active');
        $mobileClose.addClass('active');
        $navWrapper.addClass('active');
        $heroNode.addClass('active');
        $mobileInput.trigger('click');
        setTimeout(function () {
          $('#search-box-autocomplete').focus();
        }, 100)
    })
